# As in all Manifest files, the [metadata] section lets the engine
# know just what this Manifest is defining.
[metadata]

# Unsurprisingly, the type of a font Manifest is 'font.'
type = font

# The 'file' is the filename.  This should generally match the
# Manifest, but is not required.
file = rpg.png

# The name is a colloquial name, used in the engine when fonts are
# picked for a given piece of text.
name = RPG

# Lastly, the width of a space is defined not by an entry in the
# font itself, but in a number of pixels to use for spacing.  This
# does /not/ include the single-pixel separation always used between
# font characters.
space_width = 4

# The [stripmap] section defines the mapping from UTF-8 characters
# to locations in the fontstrip.  The left-hand side is the
# (zero-indexed) location; the right-hand side is a space-separated
# list of characters that a given location represents.  In addition
# to the integer locations, the special entry 'unknown' gives the
# stripmap location for the character to use when no valid entry
# for a given UTF-8 character is found.
[stripmap]
0 = A
1 = B
2 = C
3 = D
4 = E
5 = F
6 = G
7 = H
8 = I
9 = J
10 = K
11 = L
